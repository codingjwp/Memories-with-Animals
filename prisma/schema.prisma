datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Species {
  species String @id
  krSpecies String
  breeds Breeds[]
  profile Profile[]
}

model Breeds {
  id Int @id @default(autoincrement())
  speciesId String
  breed String
  krBreed String
  species Species @relation(fields: [speciesId], references: [species])
}

model Profile {
  id String @id @default(uuid())
  speciesId String
  userId String
  avatar String
  nickname String
  description String @db.Text
  species Species @relation(fields: [speciesId], references: [species])
}