// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["multiSchema"]
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}

model Species {
  species String @id
  krSpecies String
  breeds Breeds[]
}

model Breeds {
  id Int @id @default(autoincrement())
  speciesId String
  breed String
  krBreed String
  species Species @relation(fields: [speciesId], references: [species]) 
}
